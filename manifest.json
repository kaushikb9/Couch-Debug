{
  "manifest_version": 2,
  "name": "CouchBugger",
  "version": "0.1",
  "author": "Kaushik Bhat",
  "content_scripts": [
    {
      "matches": [
        "http://*/_utils/*","https://*/_utils/*"
      ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "img/icon128.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "tabs"
  ]
}